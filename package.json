{
  "name": "ring-face-recognition",
  "version": "1.12.1",
  "author": "nana4rider",
  "license": "ISC",
  "main": "dist/index.js",
  "type": "module",
  "homepage": "https://github.com/nana4rider/ring-face-recognition",
  "scripts": {
    "dev": "tsx --env-file=.env src/index.ts",
    "start": "node dist/index",
    "build": "node build.js",
    "type-check": "tsc",
    "lint": "eslint --max-warnings 0 && prettier . --check",
    "lint:fix": "eslint --fix && prettier . --write",
    "test": "vitest run",
    "prepare": "husky",
    "token": "npx -p ring-client-api ring-auth-cli"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0",
      "prettier --check"
    ],
    "*.{js,json,md,yml,yaml}": "prettier --check"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.995.0",
    "dayjs": "^1.11.19",
    "envalid": "^8.1.1",
    "gm": "^1.25.1",
    "ring-client-api": "^14.3.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@eslint/js": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/gm": "^1.25.4",
    "@types/multicast-dns": "^7.2.4",
    "@types/node": "^24.10.1",
    "@types/socket.io-client": "^3.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "type-fest": "^5.4.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  }
}
