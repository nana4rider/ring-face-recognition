{
  "name": "ring-face-recognition",
  "version": "1.9.0",
  "author": "nana4rider",
  "license": "ISC",
  "main": "dist/index.js",
  "type": "module",
  "homepage": "https://github.com/nana4rider/ring-face-recognition",
  "scripts": {
    "dev": "tsx --env-file=.env src/index.ts",
    "start": "node dist/index",
    "build": "node build.js",
    "type-check": "tsc",
    "lint": "eslint --max-warnings 0 && prettier . --check",
    "lint:fix": "eslint --fix && prettier . --write",
    "test": "vitest run",
    "prepare": "husky",
    "token": "tsx --env-file=.env fetch-refresh-token.ts"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0",
      "prettier --check"
    ],
    "*.{js,json,md,yml,yaml}": "prettier --check"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.901.0",
    "dayjs": "^1.11.18",
    "envalid": "^8.1.0",
    "fastify": "^5.6.1",
    "gm": "^1.25.1",
    "ring-client-api": "^14.1.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@eslint/js": "^9.37.0",
    "@types/debug": "^4.1.12",
    "@types/gm": "^1.25.4",
    "@types/multicast-dns": "^7.2.4",
    "@types/node": "^24.6.2",
    "@types/socket.io-client": "^3.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.15",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "type-fest": "^5.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
