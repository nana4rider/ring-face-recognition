    "ring-client-api": "^13.2.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "esbuild": "^0.24.2",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "3.4.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "type-fest": "^4.31.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
