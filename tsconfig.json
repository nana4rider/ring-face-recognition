{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "baseUrl": ".",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["src/*"],
      "werift-rtp/*": ["node_modules/werift-rtp/lib/common/src"]
    },
    "skipLibCheck": true,
    "importHelpers": true
  }
}
